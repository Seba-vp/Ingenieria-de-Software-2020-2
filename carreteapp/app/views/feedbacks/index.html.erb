

<html>
<head>
<%= stylesheet_link_tag "application" %>
</head>

<body>

<header>
<h1>Todos los feedbacks</h1>
</header>

<div class="topnav">
  <a href= <%= home_path %> >Inicio</a>

  <a href= <%= home_path %> style="float:right"> Volver</a>
</div>


<div class="lista">
<div class="card">
<table>
    <thead>
        <tr>
            <th> Id </th>
            <th> Id de servicio </th>


        </tr>   
    </thead>

    <tbody>

        <% @feedbacks.each do |feedback| %>
            <tr>
            <td> <%=feedback.id %></td>
            <td> <%=feedback.service_id %></td>
            <td> <%= link_to "Ver mas", feedback_path(feedback.id) %></td>
    

            </tr>
        <% end %>


    </tbody>
</table>
</div>

<div class="card">
<p> <%= link_to "Agregar un feedback", feedbacks_new_path%> </p>
<p> <%= link_to "Volver a inicio", home_path %> </p>
</div>
</div>





</body>
</html>


